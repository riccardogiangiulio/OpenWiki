generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  firstName String
  lastName  String
  email     String @unique
  password  String
}

model Article {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  text      String
  url       String
  pageId    Int     @unique
  mainImage String?
  userId    String
}
